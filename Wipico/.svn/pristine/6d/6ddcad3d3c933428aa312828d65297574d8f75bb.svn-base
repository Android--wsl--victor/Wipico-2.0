package com.chinasvc.wipico.type;

/**
 * 键盘
 * */
public class Keyboard {

	public static int CONTROL_KEYBOARD = 0x888B;

	/** 提交数据 */
	public static final int KEYBOARD_SUBMIT = 0x2001;
	/** 删除数据 */
	public static final int KEYBOARD_DELETE = 0x2002;

	/** 键盘提交广播Action */
	public static final String ACTION_KEYBOARD_SUBMIT = "com.keyboard.KEYBOARD_SUBMIT";

	/** 键盘删除广播Action */
	public static final String ACTION_KEYBOARD_DELETE = "com.keyboard.KEYBOARD_DELETE";

	/** 写入的值的键 */
	public static final String KEY_PARAM = "key_value";

}
